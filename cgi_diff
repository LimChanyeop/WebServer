=================good=======================

cgi: CONTENT_TYPE=text/plain
cgi: DOCUMENT_ROOT=./www/php/php-cgi
cgi: DOCUMENT_URI=/www/php/file.php
cgi: GATEWAY_INTERFACE=CGI/1.1
cgi: QUERY_STRING=
cgi: REDIRECT_STATUS=200
cgi: REMOTE_ADDR=127.0.0.1
cgi: REQUEST_METHOD=GET
cgi: REQUEST_SCHEME=GET
cgi: REQUEST_URI=/www/php/file.php
cgi: SCRIPT_FILENAME=./www/php/file.php
cgi: SCRIPT_NAME=/www/php/file.php
cgi: SERVER_NAME=localhost
cgi: SERVER_PORT=4242
cgi: SERVER_PROTOCOL=HTTP/1.1
cgi: SERVER_SOFTWARE=nginx server designed by sma