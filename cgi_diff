cgi: CONTENT_TYPE=text/plain
cgi: DOCUMENT_ROOT=./cgiBinary/php-cgi
cgi: DOCUMENT_URI=/View/file.php
cgi: GATEWAY_INTERFACE=CGI/1.1
cgi: QUERY_STRING=
cgi: REDIRECT_STATUS=200
cgi: REMOTE_ADDR=0.0.0.0
cgi: REQUEST_METHOD=GET
cgi: REQUEST_SCHEME=GET
cgi: REQUEST_URI=/View/file.php
cgi: SCRIPT_FILENAME=./View/file.php
cgi: SCRIPT_NAME=/View/file.php
cgi: SERVER_NAME=hi
cgi: SERVER_PORT=4242
cgi: SERVER_PROTOCOL=HTTP/1.1
cgi: SERVER_SOFTWARE=nginx server

