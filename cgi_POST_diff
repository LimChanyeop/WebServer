cgi: CONTENT_TYPE=multipart/form-data; boundary=----WebKitFormBoundary0sOcRgjUc74TZtPi
cgi: DOCUMENT_ROOT=./www/php/php-cgi
cgi: DOCUMENT_URI=/View/file.php
cgi: GATEWAY_INTERFACE=CGI/1.1
cgi: QUERY_STRING=
